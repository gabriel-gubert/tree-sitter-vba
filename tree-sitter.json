{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "vba",
      "camelcase": "no",
      "title": "no",
      "scope": "source.vba",
      "file-types": [
        "frm",
        "bas",
        "cls",
        "ctl"
      ],
      "injection-regex": "^vba$",
      "class-name": "TreeSitterVba"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "no",
    "authors": [
      {
        "name": "Gabriel Vitor Klaumann Gubert",
        "email": "gvkg97@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/gabriel-gubert/tree-sitter-vba"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}